#!/bin/bash

kill -SIGINT $(ps aux | grep -e gui_gateway | awk '{ print $2 }')
kill -SIGINT $(ps aux | grep -e diagnostic_manager | awk '{ print $2 }')
kill -SIGINT $(ps aux | grep -e odin_gui | awk '{ print $2 }')

sleep 2

kill -SIGTERM $(ps aux | grep -e gui_gateway | awk '{ print $2 }')
kill -SIGTERM $(ps aux | grep -e diagnostic_manager | awk '{ print $2 }')
kill -SIGTERM $(ps aux | grep -e odin_gui | awk '{ print $2 }')

# Remove from /dev/shm
rm -rf /dev/shm/*
